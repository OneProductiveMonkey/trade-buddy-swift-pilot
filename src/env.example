
# Copy this file to .env.local and fill in your actual API keys
# NEVER commit your actual API keys to version control

# Backend API Configuration
VITE_API_URL=http://localhost:5000

# Binance API Keys (Required for live trading)
# Get these from your Binance account: https://www.binance.com/en/my/settings/api-management
VITE_BINANCE_API_KEY=your_binance_api_key_here
VITE_BINANCE_SECRET=your_binance_secret_here

# Optional: Set to 'true' to use Binance testnet instead of live trading
VITE_BINANCE_SANDBOX=true

# Optional: Enable additional logging
VITE_DEBUG=false
